<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxScrollingDialog" name="PM5SettingsDialog">
		<title>Premake5 exporter settings</title>
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<object class="wxBoxSizer" variable="BoxSizer1" member="no">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizer2" member="no">
					<object class="sizeritem">
						<object class="wxStaticBoxSizer" variable="StaticBoxSizer2" member="no">
							<label>File masks</label>
							<object class="sizeritem">
								<object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="FileMasks" member="yes">
									<size>-1,200</size>
									<style>wxTE_MULTILINE</style>
								</object>
								<flag>wxALL|wxEXPAND</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="spacer">
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<size>0,400</size>
							</object>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>2</option>
					</object>
					<object class="sizeritem">
						<object class="wxNotebook" name="ID_NOTEBOOK1" variable="Notebook1" member="no">
							<size>200,0</size>
							<object class="notebookpage">
								<object class="wxPanel" name="ID_PANEL1" variable="PanelGeneral" member="no">
									<object class="wxBoxSizer" variable="BoxSizer3" member="no">
										<orient>wxVERTICAL</orient>
										<object class="sizeritem">
											<object class="wxCheckBox" name="ID_CHECKBOX1" variable="export_on_build" member="yes">
												<label>Automatic export to premake5 on build (change requires C::B restart)</label>
											</object>
											<flag>wxALL|wxALIGN_LEFT</flag>
											<border>5</border>
										</object>
										<object class="sizeritem">
											<object class="wxCheckBox" name="ID_CHECKBOX2" variable="use_workspace_prefix" member="yes">
												<label>Use workspace prefix in premake5 filename</label>
											</object>
											<flag>wxALL|wxALIGN_LEFT</flag>
											<border>5</border>
										</object>
										<object class="sizeritem">
											<object class="wxStaticBoxSizer" variable="StaticBoxSizer6" member="no">
												<label>Use defaults for</label>
												<object class="sizeritem">
													<object class="wxCheckBox" name="ID_CHECKBOX3" variable="use_workspace_defaults" member="yes">
														<label>Workspace</label>
													</object>
													<flag>wxALL|wxEXPAND</flag>
													<border>5</border>
												</object>
												<object class="sizeritem">
													<object class="wxCheckBox" name="ID_CHECKBOX4" variable="use_project_defaults" member="yes">
														<label>Project</label>
													</object>
													<flag>wxALL|wxEXPAND</flag>
													<border>5</border>
												</object>
											</object>
											<flag>wxALL|wxALIGN_LEFT</flag>
											<border>5</border>
										</object>
										<object class="sizeritem">
											<object class="wxStaticBoxSizer" variable="StaticBoxSizer5" member="no">
												<label>Export project types</label>
												<object class="sizeritem">
													<object class="wxCheckBox" name="ID_CHECKBOX5" variable="ExportWindowedApp" member="yes">
														<label>WindowedApp</label>
													</object>
													<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
												</object>
												<object class="sizeritem">
													<object class="wxCheckBox" name="ID_CHECKBOX6" variable="ExportConsoleApp" member="yes">
														<label>ConsoleApp</label>
													</object>
													<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
												</object>
												<object class="sizeritem">
													<object class="wxCheckBox" name="ID_CHECKBOX7" variable="ExportStaticLib" member="yes">
														<label>StaticLib</label>
													</object>
													<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
												</object>
												<object class="sizeritem">
													<object class="wxCheckBox" name="ID_CHECKBOX8" variable="ExportSharedLib" member="yes">
														<label>SharedLib</label>
													</object>
													<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
												</object>
											</object>
											<flag>wxALL|wxALIGN_LEFT</flag>
											<border>5</border>
										</object>
										<object class="sizeritem">
											<object class="wxCheckBox" name="ID_CHECKBOX11" variable="export_cb_targetname" member="yes">
												<label>Export C::B build target output file names</label>
											</object>
											<flag>wxALL|wxALIGN_LEFT</flag>
											<border>5</border>
											<option>1</option>
										</object>
										<object class="sizeritem">
											<object class="wxCheckBox" name="ID_CHECKBOX10" variable="post_build_copy" member="yes">
												<label>Post build: Copy binaries to common folders under workspace</label>
											</object>
											<flag>wxALL|wxALIGN_LEFT</flag>
											<border>5</border>
										</object>
										<object class="sizeritem">
											<object class="wxCheckBox" name="ID_CHECKBOX9" variable="save_all_on_export" member="yes">
												<label>Save C::B workspace and projects on export</label>
											</object>
											<flag>wxALL|wxALIGN_LEFT</flag>
											<border>5</border>
										</object>
										<object class="spacer">
											<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
											<border>5</border>
											<option>1</option>
										</object>
										<object class="sizeritem">
											<object class="wxStaticBoxSizer" variable="StaticBoxSizer4" member="no">
												<label>Factory settings</label>
												<object class="sizeritem">
													<object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText3" member="yes">
														<label>Warning: This cannot be undone!</label>
													</object>
													<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
													<option>1</option>
												</object>
												<object class="sizeritem">
													<object class="wxButton" name="ID_BUTTON1" variable="FactorySettingsButton" member="no">
														<label>Restore factory settings</label>
														<handler function="OnFactorySettingsButtonClick" entry="EVT_BUTTON" />
													</object>
													<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
												</object>
											</object>
											<flag>wxALL|wxEXPAND</flag>
											<border>5</border>
										</object>
									</object>
								</object>
								<label>General</label>
							</object>
							<object class="notebookpage">
								<object class="wxPanel" name="ID_PANEL4" variable="PanelWorkspaceDefaults" member="no">
									<object class="wxBoxSizer" variable="BoxSizer9" member="no">
										<orient>wxVERTICAL</orient>
										<object class="sizeritem">
											<object class="wxBoxSizer" variable="BoxSizer10" member="no">
												<object class="sizeritem">
													<object class="wxStaticText" name="wxID_ANY" variable="StaticText4" member="no">
														<label>Settings apply to premake workspace level</label>
													</object>
													<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
													<option>1</option>
												</object>
											</object>
											<flag>wxEXPAND</flag>
											<border>5</border>
										</object>
										<object class="sizeritem">
											<object class="wxTextCtrl" name="ID_TEXTCTRL5" variable="workspace_defaults" member="yes">
												<style>wxTE_MULTILINE</style>
											</object>
											<flag>wxALL|wxEXPAND</flag>
											<border>5</border>
											<option>1</option>
										</object>
									</object>
								</object>
								<label>Workspace defaults</label>
							</object>
							<object class="notebookpage">
								<object class="wxPanel" name="ID_PANEL3" variable="Panelproject_defaults" member="no">
									<object class="wxBoxSizer" variable="BoxSizer5" member="no">
										<orient>wxVERTICAL</orient>
										<object class="sizeritem">
											<object class="wxBoxSizer" variable="BoxSizer8" member="no">
												<object class="sizeritem">
													<object class="wxStaticText" name="wxID_ANY" variable="StaticText2" member="no">
														<label>Settings apply to premake project level</label>
													</object>
													<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
													<option>1</option>
												</object>
											</object>
											<flag>wxEXPAND</flag>
											<border>5</border>
										</object>
										<object class="sizeritem">
											<object class="wxTextCtrl" name="ID_TEXTCTRL4" variable="project_defaults" member="yes">
												<style>wxTE_MULTILINE</style>
											</object>
											<flag>wxALL|wxEXPAND</flag>
											<border>5</border>
											<option>1</option>
										</object>
									</object>
								</object>
								<label>Project defaults</label>
							</object>
							<object class="notebookpage">
								<object class="wxPanel" name="ID_PANEL2" variable="PanelConfigDefaults" member="no">
									<object class="wxBoxSizer" variable="BoxSizer4" member="no">
										<orient>wxVERTICAL</orient>
										<object class="sizeritem">
											<object class="wxBoxSizer" variable="BoxSizer7" member="no">
												<object class="sizeritem">
													<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
														<label>Settings apply to premake configurations filters</label>
													</object>
													<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
													<option>1</option>
												</object>
											</object>
											<flag>wxEXPAND</flag>
											<border>5</border>
										</object>
										<object class="sizeritem">
											<object class="wxBoxSizer" variable="BoxSizer6" member="no">
												<object class="sizeritem">
													<object class="wxStaticBoxSizer" variable="StaticBoxSizer3" member="no">
														<label>Debug</label>
														<object class="sizeritem">
															<object class="wxTextCtrl" name="ID_TEXTCTRL3" variable="configurations_debug" member="yes">
																<style>wxTE_MULTILINE</style>
															</object>
															<flag>wxALL|wxEXPAND</flag>
															<border>5</border>
															<option>1</option>
														</object>
													</object>
													<flag>wxALL|wxEXPAND</flag>
													<border>5</border>
													<option>1</option>
												</object>
												<object class="sizeritem">
													<object class="wxStaticBoxSizer" variable="StaticBoxSizer1" member="no">
														<label>Release</label>
														<object class="sizeritem">
															<object class="wxTextCtrl" name="ID_TEXTCTRL2" variable="configurations_release" member="yes">
																<style>wxTE_MULTILINE</style>
															</object>
															<flag>wxALL|wxEXPAND</flag>
															<border>5</border>
															<option>1</option>
														</object>
													</object>
													<flag>wxALL|wxEXPAND</flag>
													<border>5</border>
													<option>1</option>
												</object>
											</object>
											<flag>wxALL|wxEXPAND</flag>
											<border>5</border>
											<option>1</option>
										</object>
									</object>
								</object>
								<label>Config defaults</label>
							</object>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>6</option>
					</object>
				</object>
				<flag>wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="spacer">
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<size>900,0</size>
			</object>
			<object class="sizeritem">
				<object class="wxStdDialogButtonSizer" variable="StdDialogButtonSizer1" member="no">
					<object class="button">
						<object class="wxButton" name="wxID_OK">
							<label></label>
						</object>
					</object>
					<object class="button">
						<object class="wxButton" name="wxID_CANCEL">
							<label></label>
						</object>
					</object>
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
			</object>
		</object>
	</object>
</wxsmith>
